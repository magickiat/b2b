<%@taglib prefix="c" uri="http://java.sun.com/jsp/jstl/core"%>
<%@page session="true"%>

<c:forEach var="row" items="${ productListAll }" varStatus="rowNum">
	<%-- Declare product variable --%>
	<c:set var="product" value="${ row.product }" />
	<div
		class="${ row.keyName }${ product.productLength}_${rowNum.index} text-center"
		style='<c:if test="${rowNum.index > 0}">display: none;</c:if>'>
		<h3>${ product.productNameEn }</h3>
	</div>
</c:forEach>